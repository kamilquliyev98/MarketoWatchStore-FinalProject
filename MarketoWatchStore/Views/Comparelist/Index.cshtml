@model IEnumerable<CompareListVM>
@{
    ViewBag.Title = "Marketo | Compare List";
}

<!-- Breadcrumb section start -->
<section class="breadcrumb-section section-b-space">
    <ul class="circles">
        @for (int i = 1; i <= 10; i++)
        {
            <li></li>
        }
    </ul>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h3>Compare list</h3>
                <nav>
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a asp-controller="home" asp-action="index">
                                <i class="fas fa-home"></i>
                            </a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">Compare list</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</section>
<!-- Breadcrumb section end -->
<!-- Compare Section Start -->
<section class="section-b-space">
    <div class="container">
        <div id="comparelist-table">

            @await Html.PartialAsync("_CompareTablePartial", Model)

        </div>
    </div>
</section>
<!-- Compare Section End -->
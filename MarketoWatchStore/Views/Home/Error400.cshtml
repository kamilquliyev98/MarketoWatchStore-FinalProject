@{
    ViewBag.Title = "ERROR 400 | Marketo";
}

<!-- Breadcrumb section start -->
<section class="breadcrumb-section section-b-space">
    <ul class="circles">
        @for (int i = 1; i <= 10; i++)
        {
            <li></li>
        }
    </ul>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h3>Bad Request - ERROR 400</h3>
            </div>
        </div>
    </div>
</section>
<!-- Breadcrumb section end -->

<!-- 400 Section Start -->
<section class="page-not-found section-b-space">
    <div class="container">
        <div class="row gx-md-2 gx-0 gy-md-0 gy-3">
            <div class="col-md-8 m-auto">
                <div class="page-image">
                    <img src="~/assets/images/inner-page/400.jpg" class="img-fluid blur-up lazyload" alt="400">
                </div>
            </div>

            <div class="col-md-8 mx-auto mt-md-5 mt-3">
